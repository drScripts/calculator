{
  "expo": {
    "name": "calculator",
    "slug": "calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/calculator.png",
    "splash": {
      "image": "./assets/calculator-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFA0A0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/calculator.png",
        "backgroundColor": "#FFA0A0"
      },
      "package": "com.drscripts.calculator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
